{
  "name": "jsreport-fs-store",
  "version": "2.3.1",
  "description": "File system based templates store",
  "scripts": {
    "start": "jsreport-studio-start --httpPort 5488",
    "run": "npm run build && jsreport-studio-start --httpPort 5488 --run-only",
    "test": "set DEBUG=jsreport && mocha --timeout 7000 test &&standard",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-fs-store",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-fs-store.git"
  },
  "keywords": [
    "jsreport",
    "pdf"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "engines": {
    "node": ">=8.9"
  },
  "license": "MIT",
  "dependencies": {
    "bluebird": "3.5.3",
    "chokidar": "2.0.4",
    "lockfile": "1.0.4",
    "lodash.isequal": "4.5.0",
    "lodash.omit": "4.5.0",
    "mingo": "2.2.8",
    "mkdirp": "0.5.1",
    "node.extend.without.arrays": "1.1.5",
    "rimraf": "2.6.2",
    "socket.io": "2.1.1",
    "uuid": "3.3.2"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "9.0.0",
    "eslint": "5.6.0",
    "eslint-config-standard-jsx": "6.0.2",
    "eslint-plugin-babel": "5.2.0",
    "in-publish": "2.0.0",
    "jsreport-core": "2.3.0",
    "jsreport-express": "2.2.3",
    "jsreport-studio-dev": "1.3.2",
    "jsreport-templates": "2.1.0",
    "mocha": "5.2.0",
    "ncp": "2.0.0",
    "should": "13.2.3",
    "should-sinon": "0.0.6",
    "sinon": "6.3.4",
    "socket.io-client": "2.1.1",
    "standard": "12.0.1"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
