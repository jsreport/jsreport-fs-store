{
  "name": "jsreport-fs-store",
  "version": "1.0.4",
  "description": "jsreport extension adding support for storing reports on the file system",
  "main": "index.js",
  "scripts": {
    "test": "mocha test && standard",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-fs-store",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-fs-store.git"
  },
  "keywords": [
    "jsreport",
    "store"
  ],
  "standard": {
    "ignore": [
      "persistence.js",
      "studio/main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "dependencies": {
    "async": "1.5.2",
    "chokidar": "1.5.2",
    "fs-extra": "0.30.0",
    "glob": "7.0.5",
    "in-publish": "2.0.0",
    "listener-collection": "0.1.0",
    "mkdirp": "0.5.1",
    "nedb": "1.8.0",
    "node.extend": "1.1.5",
    "q": "1.4.1",
    "socket.io": "1.4.6",
    "underscore": "1.8.3"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "6.0.5",
    "eslint": "2.13.1",
    "eslint-config-standard-jsx": "1.2.1",
    "eslint-plugin-babel": "3.2.0",
    "jsreport-studio-dev": "1.0.0",
    "mocha": "2.5.3",
    "should": "9.0.2",
    "standard": "7.1.2"
  },
  "files": [
    "lib", "index.js", "jsreport.config.js", "studio"
  ]
}
