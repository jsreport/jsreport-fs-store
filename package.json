{
  "name": "jsreport-fs-store",
  "version": "1.2.1",
  "description": "jsreport extension adding support for storing reports on the file system",
  "main": "index.js",
  "scripts": {
    "test": "mocha test && standard",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-fs-store",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-fs-store.git"
  },
  "keywords": [
    "jsreport",
    "store"
  ],
  "standard": {
    "ignore": [
      "persistence.js",
      "deepCopy.js",
      "studio/main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "dependencies": {
    "async": "2.3.0",
    "bluebird": "3.5.0",
    "chokidar": "1.6.1",
    "fs-extra": "2.1.2",
    "in-publish": "2.0.0",
    "listener-collection": "1.0.0",
    "mkdirp": "0.5.1",
    "nedb": "1.8.0",
    "node.extend": "1.1.6",
    "socket.io": "1.7.3",
    "underscore": "1.8.3"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "7.2.2",
    "eslint": "3.19.0",
    "eslint-config-standard-jsx": "3.3.0",
    "eslint-plugin-babel": "4.1.1",
    "jsreport-studio-dev": "1.1.0",
    "mocha": "3.2.0",
    "should": "11.2.1",
    "standard": "9.0.1"
  },
  "files": [
    "lib",
    "index.js",
    "jsreport.config.js",
    "studio"
  ]
}
