{
  "name": "jsreport-fs-store",
  "version": "2.7.0",
  "description": "File system based templates store",
  "scripts": {
    "start": "jsreport-studio-start --httpPort 5488",
    "run": "npm run build && jsreport-studio-start --httpPort 5488 --run-only",
    "test": "set DEBUG=jsreport && mocha --timeout 15000 test &&standard",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-fs-store",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-fs-store.git"
  },
  "keywords": [
    "jsreport",
    "pdf"
  ],
  "standard": {
    "ignore": [
      "studio/main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "engines": {
    "node": ">=8.9"
  },
  "license": "MIT",
  "dependencies": {
    "@jsreport/mingo": "2.4.1",
    "bluebird": "3.7.2",
    "chokidar": "3.3.1",
    "lockfile": "1.0.4",
    "lodash.debounce": "4.0.8",
    "lodash.omit": "4.5.0",
    "mkdirp": "0.5.1",
    "node.extend.without.arrays": "1.1.6",
    "rimraf": "3.0.2",
    "socket.io": "2.3.0",
    "uuid": "3.3.3"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "eslint": "6.7.2",
    "eslint-config-standard-jsx": "8.1.0",
    "eslint-plugin-babel": "5.3.0",
    "in-publish": "2.0.0",
    "jsreport-core": "2.7.0",
    "jsreport-express": "2.6.0",
    "jsreport-studio-dev": "1.5.0",
    "jsreport-templates": "2.3.2",
    "mocha": "6.2.2",
    "ncp": "2.0.0",
    "should": "13.2.3",
    "should-sinon": "0.0.6",
    "sinon": "7.5.0",
    "socket.io-client": "2.3.0",
    "standard": "14.3.1"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
