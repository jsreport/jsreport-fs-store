{
  "name": "jsreport-fs-store",
  "version": "2.0.4",
  "description": "File system based templates store",
  "scripts": {
    "start": "jsreport-studio-start --httpPort 5488",
    "run": "npm run build && jsreport-studio-start --httpPort 5488 --run-only",
    "test": "set DEBUG=jsreport&&mocha test&&standard",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-fs-store",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-fs-store.git"
  },
  "keywords": [
    "jsreport",
    "pdf"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "chokidar": "2.0.2",
    "lockfile": "1.0.3",
    "lodash.omit": "4.5.0",
    "mingo": "2.2.0",
    "mkdirp": "0.5.1",
    "node.extend": "2.0.0",
    "rimraf": "2.6.2",
    "socket.io": "2.0.4",
    "uuid": "3.2.1"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "eslint": "4.19.0",
    "eslint-config-standard-jsx": "5.0.0",
    "eslint-plugin-babel": "4.1.2",
    "in-publish": "2.0.0",
    "jsreport-core": "jsreport/jsreport-core#2.0.0-beta",
    "jsreport-express": "jsreport/jsreport-express#2.0.0-beta",
    "jsreport-studio-dev": "1.1.2",
    "jsreport-templates": "jsreport/jsreport-templates#2.0.0-beta",
    "mocha": "5.0.4",
    "ncp": "2.0.0",
    "should": "13.2.1",
    "should-sinon": "0.0.6",
    "sinon": "4.4.6",
    "socket.io-client": "2.0.4",
    "standard": "11.0.1"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
